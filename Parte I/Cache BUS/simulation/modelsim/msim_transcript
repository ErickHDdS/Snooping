add wave -position end  sim:/MaquinaEstadoBUS/invalid
add wave -position end  sim:/MaquinaEstadoBUS/exclusive
add wave -position end  sim:/MaquinaEstadoBUS/shared
add wave -position end  sim:/MaquinaEstadoBUS/read_miss
add wave -position end  sim:/MaquinaEstadoBUS/write_miss
